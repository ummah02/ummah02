from email import message
import telebot
from telebot import types 
from telebot.types import LabeledPrice


bot = telebot.TeleBot("")
TO_CHAT_ID = "" 
provider_token_click = ''
provider_token_payme = ''

prices1 = [LabeledPrice(label='–û–î–ù–û–ú–ï–°–¢–ù–´–ô –ù–û–ú–ï–†!', amount=110000)]
prices_1 = [LabeledPrice(label='–û–î–ù–û–ú–ï–°–¢–ù–´–ô –ù–û–ú–ï–†!', amount=110000)]
prices2 = [LabeledPrice(label='–î–í–£–•–ú–ï–°–¢–ù–´–ô –°–¢–ê–ù–î–ê–†–¢!', amount=36000000)]
prices3 = [LabeledPrice(label='–î–í–£–•–ú–ï–°–¢–ù–´–ô –ü–û–õ–£ –õ–Æ–ö–°!', amount=39000000)]
prices4 = [LabeledPrice(label='–¢–†–ï–•–ú–ï–°–¢–ù–´–ô –°–¢–ê–ù–î–ê–†–¢!', amount=51500000)]
prices5 = [LabeledPrice(label='–°–ï–ú–ï–ô–ù–´–ô –õ–Æ–ö–° –ù–û–ú–ï–†!', amount=57000000)]
prices6 = [LabeledPrice(label='–õ–Æ–ö–° –° –ë–ê–õ–ö–û–ù–û–ú!', amount=57000000)]

@bot.message_handler(commands=['–ë–†–û–ù–ò–†–û–í–ê–ù–ò–ï-1Ô∏è‚É£','SINGLE_ROOM']  )
def user_1(m, res=False):
    –∞–Ω–∫–µ—Ç–∞=bot.send_message(m.chat.id, "<i><b>–ë–†–û–ù–ò–†–û–í–ê–ùO:\n<ins>SINGLE ROOM\–û–î–ù–û–ú–ï–°–¢–ù–´–ô –ù–û–ú–ï–†!</ins>\n\n""*–û–¢–ü–†–ê–í–¨–¢–ï –ù–ê–ú –í–ê–® –§.–ò.–û.!\n*–°–ï–†–ò–Ø –ü–ê–°–ü–û–†–¢–ê!\n*–¢–ï–õ–ï–§–û–ù –ù–û–ú–ï–†!\n*–ò –ù–ê –ö–ê–ö–ò–ï –î–ê–¢–´?\n\n*–ö –ü–†–ò–ú–ï–†–£:</b></i>\n<ins><i>-–ê–ë–î–£–õ–õ–ê–ï–í –ù–û–î–ò–† –û–î–ò–õ–û–í–ò–ß\n-AB240802\n-(90)1112233\n-–û–¢ 1 –°–ï–ù–¢–Ø–ë–†–Ø, –î–û 2–≥–æ –°–ï–ù–¢–Ø–ë–†–Ø</i></ins>",parse_mode="html")
    bot.register_next_step_handler(–∞–Ω–∫–µ—Ç–∞, save_1)
    bot.forward_message(TO_CHAT_ID, m.chat.id, message.id)
def save_1(message, res=False):
    –ø—Ä–∏–º–µ—á–∞–Ω–∏–µ = bot.send_message(message.from_user.id, "<b>–ú–û–ñ–ï–¢–ï –û–¢–ü–†–ê–í–ò–¢–¨ –ù–ê–ú\n–î–û–ü–û–õ–¨–ù–ò–¢–ï–õ–¨–ù–£–Æ –ò–ù–§–û–†–ú–ê–¶–ò–Æ!</b>\n\n–ö –ü–†–ò–ú–ï–†–£:\n\n<i>-–°–æ–µ–¥–∏–Ω–∏—Ç—å –∫—Ä–æ–≤–∞—Ç—å\n-–í–∫–ª—é—á–∏—Ç–µ –∫–æ–Ω–¥–∏—Ç—Ü–∏–æ–Ω–µ—Ä –¥–æ –Ω–∞—à–µ–≥–æ –ø—Ä–∏—Ö–æ–¥–∞</i>",parse_mode="html")
    bot.register_next_step_handler(–ø—Ä–∏–º–µ—á–∞–Ω–∏–µ, search_1)
    bot.forward_message(TO_CHAT_ID, message.from_user.id, message.id)
def search_1(message, res=False):
    bot.send_message(message.from_user.id, "<b>–í—ã–±–µ—Ä–∏—Ç–µ –ü–õ–ê–¢–§–û–†–ú–£ –¥–ª—è —Ä–µ–∞–ª–∏–∑–∞—Ü–∏–∏ –æ–ø–ª–∞—Ç—ã!</b>\n\n"
                                            "*****<ins><b>/CLICK_1 –∏–ª–∏ /PAYME_1</b></ins>*****",parse_mode="html")
    markup = types.ReplyKeyboardMarkup(row_width=1)
    btn1 = types.KeyboardButton("/CLICK_1")
    btn2 = types.KeyboardButton("/PAYME_1")
    markup.add(btn1,btn2,)
    bot.send_message(message.chat.id, text="<b>–ù–ê–ñ–ú–ò–¢–ï –ù–ê –ö–ù–û–ü–ö–£!</b>",reply_markup=markup, parse_mode="html")
    bot.forward_message(TO_CHAT_ID, message.from_user.id, message.id)

#----------------------------------------------------------------------------------------------------------------------

@bot.message_handler(commands=['CLICK_1'])  
def –ø–ª–∞—Ç–µ–∂_1(message, res=False):
    bot.send_message(message.chat.id,
                     "–í—ã –≤—ã–±—Ä–∞–ª–∏ –ø–ª–∞—Ç—Ñ–æ—Ä–º—É *CLICK*‚úÖ" , parse_mode = 'Markdown' )
    bot.send_invoice(message.chat.id,  #chat_id
                     '–ë–†–û–ù–ò–†–û–í–ê–ù–ò–ï –ù–û–ú–ï–†–ê!', #title
                     '–°–†–û–ö –ò–°–¢–ï–ö–ê–ù–ò–ï-00:00', #description
                     '!_!', #invoice_payload
                     provider_token_click, #provider_token
                     'UZS', #currency
                     prices1, #prices
                     photo_url=("https://hitfm.ru/rx/750x750,c_1,g_CenterCenter/uploads/6a/97/d59d8f14d7a3f0d59d805f03354b.jpg"),
                     photo_height=512,
                     photo_width=512,
                     photo_size=512,
                     is_flexible=False,
                     start_parameter='hotel')
    markup = types.ReplyKeyboardMarkup(row_width=1)
    btn1 = types.KeyboardButton("–ì–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é‚è™")
    markup.add(btn1,)
    bot.send_message(message.chat.id, text="<i><b>–ù–ê–ñ–ú–ò–¢–ï –ù–ê –ö–ù–û–ü–ö–£ –ó–ê–ü–õ–ê–¢–ò–¢–¨!</b></i>",reply_markup=markup, parse_mode="html")
    bot.forward_message(TO_CHAT_ID, message.from_user.id, message.id)  
@bot.pre_checkout_query_handler(func=lambda query: True)
def checkout(pre_checkout_query):
    bot.answer_pre_checkout_query(pre_checkout_query.id, ok=True,
                                  error_message="–ü—Ä–æ—Å–∏–º –ø—Ä–æ—à–µ–Ω–∏–µ! –ü–†–û–ò–ó–û–®–õ–û –ú–ê–õ–ï–ù–¨–ù–û–ï –û–®–ò–ë–ö–ê! –ü–æ–ø—Ä–æ–±—É–π—Ç–µ –∑–∞–ø–ª–∞—Ç–∏—Ç—å –µ—â–µ —Ä–∞–∑ —á–µ—Ä–µ–∑ –Ω–µ—Å–∫–æ–ª—å–∫–æ –º–∏–Ω—É—Ç ")
@bot.message_handler(content_types=['successful_payment'])
def got_payment(message):
    bot.send_message(message.chat.id,'–ü–õ–ê–¢–ï–ñ –£–°–ü–ï–®–ù–û –ü–†–û–í–ï–î–Å–ù –í –†–ê–ó–ú–ï–†–ï! ***{} {}*** –ñ–î–ï–ú –í–ê–° –í –ì–û–°–¢–ò!'.format(message.successful_payment.total_amount / 100, message.successful_payment.currency),parse_mode='Markdown')

