@bot.message_handler(commands=['CLICK_6'])  
def –ø–ª–∞—Ç–µ–∂_6(message, res=False):
        bot.send_message(message.chat.id,
                        "–í—ã –≤—ã–±—Ä–∞–ª–∏ –ø–ª–∞—Ç—Ñ–æ—Ä–º—É *CLICK*‚úÖ" , parse_mode = 'Markdown' )
        bot.send_invoice(message.chat.id,  #chat_id
                        '–ë–†–û–ù–ò–†–û–í–ê–ù–ò–ï –ù–û–ú–ï–†–ê!', #title
                        '–°–†–û–ö –ò–°–¢–ï–ö–ê–ù–ò–ï-00:00', #description
                        '!_!', #invoice_payload
                        provider_token_click, #provider_token
                        'UZS', #currency
                        prices6, #prices
                        photo_url=("https://hitfm.ru/rx/750x750,c_1,g_CenterCenter/uploads/6a/97/d59d8f14d7a3f0d59d805f03354b.jpg"),
                        photo_height=512,
                        photo_width=512,
                        photo_size=512,
                        is_flexible=False,
                        start_parameter='hotel')
        markup = types.ReplyKeyboardMarkup(row_width=1)
        btn1 = types.KeyboardButton("–ì–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é‚è™")
        markup.add(btn1,)
        bot.send_message(message.chat.id, text="<i><b>–ù–ê–ñ–ú–ò–¢–ï –ù–ê –ö–ù–û–ü–ö–£ –ó–ê–ü–õ–ê–¢–ò–¢–¨!</b></i>",reply_markup=markup, parse_mode="html")
        bot.forward_message(TO_CHAT_ID, message.from_user.id, message.id)  
