@bot.message_handler(commands=['–ë–†–û–ù–ò–†–û–í–ê–ù–ò–ï-6Ô∏è‚É£','SUITE_WITH_BALCONY'])
def start_6(message, res=False):   
        –∞–Ω–∫–µ—Ç–∞=bot.send_message(message.from_user.id, "<i><b>–ë–†–û–ù–ò–†–û–í–ê–ùO:\n<ins>–õ–Æ–ö–° –° –ë–ê–õ–ö–û–ù–û–ú\–°–ï–ú–ï–ô–ù–´–ô –õ–Æ–ö–° –ù–û–ú–ï–†!</ins>\n\n""*–û–¢–ü–†–ê–í–¨–¢–ï –ù–ê–ú –í–ê–® –§.–ò.–û.!\n*–°–ï–†–ò–Ø –ü–ê–°–ü–û–†–¢–ê!\n*–¢–ï–õ–ï–§–û–ù –ù–û–ú–ï–†!\n*–ò –ù–ê –ö–ê–ö–ò–ï –î–ê–¢–´?\n\n*–ö –ü–†–ò–ú–ï–†–£:</b></i>\n<ins><i>-–ê–ë–î–£–õ–õ–ê–ï–í –ù–û–î–ò–† –û–î–ò–õ–û–í–ò–ß\n-AB240802\n-(90)1112233\n-–û–¢ 1 –°–ï–ù–¢–Ø–ë–†–Ø, –î–û 2–≥–æ –°–ï–ù–¢–Ø–ë–†–Ø</i></ins>",parse_mode="html")
        bot.register_next_step_handler(–∞–Ω–∫–µ—Ç–∞, save_6)
        bot.forward_message(TO_CHAT_ID, message.from_user.id, message.id)
def save_6(message, res=False):
        –ø—Ä–∏–º–µ—á–∞–Ω–∏–µ = bot.send_message(message.from_user.id, "<b>–ú–û–ñ–ï–¢–ï –û–¢–ü–†–ê–í–ò–¢–¨ –ù–ê–ú\n–î–û–ü–û–õ–¨–ù–ò–¢–ï–õ–¨–ù–£–Æ –ò–ù–§–û–†–ú–ê–¶–ò–Æ!</b>\n\n–ö –ü–†–ò–ú–ï–†–£:\n\n<i>-–°–æ–µ–¥–∏–Ω–∏—Ç—å –∫—Ä–æ–≤–∞—Ç—å\n-–í–∫–ª—é—á–∏—Ç–µ –∫–æ–Ω–¥–∏—Ç—Ü–∏–æ–Ω–µ—Ä –¥–æ –Ω–∞—à–µ–≥–æ –ø—Ä–∏—Ö–æ–¥–∞</i>",parse_mode="html")
        bot.register_next_step_handler(–ø—Ä–∏–º–µ—á–∞–Ω–∏–µ, search_6)
        bot.forward_message(TO_CHAT_ID, message.from_user.id, message.id)
def search_6(message, res=False):
        bot.send_message(message.from_user.id, "<b>–í—ã–±–µ—Ä–∏—Ç–µ –ü–õ–ê–¢–§–û–†–ú–£ –¥–ª—è —Ä–µ–∞–ª–∏–∑–∞—Ü–∏–∏ –æ–ø–ª–∞—Ç—ã!</b>\n\n"
                                                "*****<ins><b>/CLICK_6 –∏–ª–∏ /PAYME_6</b></ins>*****",parse_mode="html")
        markup= types.ReplyKeyboardMarkup(row_width=1)
        btn1 = types.KeyboardButton("/CLICK_6")
        btn2 = types.KeyboardButton("/PAYME_6")
        markup.add(btn1,btn2,)
        bot.send_message(message.chat.id, text="<b>–ù–ê–ñ–ú–ò–¢–ï –ù–ê –ö–ù–û–ü–ö–£!</b>",reply_markup=markup, parse_mode="html")
        bot.forward_message(TO_CHAT_ID, message.from_user.id, message.id)

