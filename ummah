@bot.message_handler(content_types=["text"])
def start(message):
    if (message.text == '/start'or message.text == "–ì–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é‚è™" ):  
        markup = types.ReplyKeyboardMarkup(row_width=1)
        btn1 = types.KeyboardButton("üîë –ù–û–ú–ï–†–ê")
        btn2 = types.KeyboardButton("–£–°–õ–£–ì–ò")
        btn3 = types.KeyboardButton("–£–°–õ–û–í–ò–Ø –ü–†–û–ñ–ò–í–ê–ù–ò–Ø")
        btn4 = types.KeyboardButton("üó∫ –õ–û–ö–ê–¶–ò–Ø")
        btn5 = types.KeyboardButton("üè® –ß–¢–û –ù–ê–•–û–î–ò–¢–°–Ø –†–Ø–î–û–ú")
        btn6 = types.KeyboardButton("üèû –ì–ê–õ–ê–†–ï–Ø!")
        btn7 = types.KeyboardButton("‚òéÔ∏è –ö–û–ù–¢–ê–ö–¢–´")
        btn8 = types.KeyboardButton("–ë–´–°–¢–†–ê–Ø –ë–†–û–ù–¨")
        markup.add(btn1,btn2,btn3,btn4,btn5,btn6,btn7,btn8)
        bot.send_message(message.chat.id, text="<b>–î–æ–±—Ä–æ –ø–æ–∂–∞–ª–æ–≤–∞—Ç—å!</b>\n*****<ins><b>{0.first_name}!</b></ins>***** ""\n\n<i>–ù–∞ –æ—Ñ–∏—Ü–∏–∞–ª—å–Ω—ã–π –±–æ—Ç –Ω–∞—à–µ–≥–æ –æ—Ç–µ–ª—è\n</i><b>HOTEL DIAMOND TASHKENT !</b>\n\n<i>–ß–¢–û –ò–ù–¢–ï–†–ï–°–£–ï–¢ –í–ê–°?</i>\n\n<b>*–ë–†–û–ù–ò–†–û–í–ê–ù–ò–ï –ù–û–ú–ï–†–û–í‚úÖ\n*–£–°–õ–£–ì–ò‚ÑπÔ∏è\n*–£–°–õ–û–í–ò–Ø –ü–†–û–ñ–ò–í–ê–ù–ò–Ø‚ùï\n*–õ–û–ö–ê–¶–ò–Øüö©\n*–ß–¢–û –ù–ê–•–û–î–ò–¢–°–Ø –†–Ø–î–û–ú!üè®\n*–ö–û–ù–¢–ê–ö–¢–´‚òéÔ∏è</b>".format(message.from_user),reply_markup=markup, parse_mode="html") 
       
    if (message.text == "üîë –ù–û–ú–ï–†–ê" or message.text == "–ù–ê–ó–ê–î!‚Ü©Ô∏è"):
        bot.send_message(message.chat.id, text="<b>–í–´–ë–ï–†–ò–¢–ï –ù–û–ú–ï–†!</b>\n\n1Ô∏è‚É£-<b>SINGLE ROOM</b>\n–û–î–ù–û–ú–ï–°–¢–ù–´–ô!\n\n2Ô∏è‚É£-<b>STANDART TWIN</b>\n–î–í–£–•–ú–ï–°–¢–ù–´–ô –°–¢–ê–ù–î–ê–†–¢!\n\n3Ô∏è‚É£-<b>DELUXE TWIN</b>\n–î–í–£–•–ú–ï–°–¢–ù–´–ô –ü–û–õ–£ –õ–Æ–ö–°!\n\n4Ô∏è‚É£-<b>TRIPLE STANDART</b>\n–¢–†–ï–•–ú–ï–°–¢–ù–´–ô –°–¢–ê–ù–î–ê–†–¢!\n\n5Ô∏è‚É£-<b>FAMILY SUITE ROOM</b>\n–°–ï–ú–ï–ô–ù–´–ô –õ–Æ–ö–° –ù–û–ú–ï–†\n\n6Ô∏è‚É£-<b>SUITE WITH BALCONY\n</b>–õ–Æ–ö–° –° –ë–ê–õ–ö–û–ù–û–ú\n\n7Ô∏è‚É£-<b>CONFERENCE HALL\n</b>–ó–ê–õ –î–õ–Ø –ö–û–ù–§–ï–†–ï–ù–¶–ò–ò",parse_mode="html")
        markup = types.ReplyKeyboardMarkup(resize_keyboard=True)
        btn1 = types.KeyboardButton("1Ô∏è‚É£")
        btn2 = types.KeyboardButton("2Ô∏è‚É£")
        btn3 = types.KeyboardButton("3Ô∏è‚É£")
        btn4 = types.KeyboardButton("4Ô∏è‚É£")
        btn5 = types.KeyboardButton("5Ô∏è‚É£")
        btn6 = types.KeyboardButton("6Ô∏è‚É£")
        btn7 = types.KeyboardButton("7Ô∏è‚É£")
        back = types.KeyboardButton("–ì–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é‚è™")
        markup.add(btn1, btn2,btn3,btn4,btn5,btn6,btn7,back)
        bot.send_message(message.chat.id, text="<b>–ù–ê–ñ–ú–ò–¢–ï –ù–ê –ö–ù–û–ü–ö–£!</b>", reply_markup=markup,parse_mode="html")
  
    elif (message.text == "üó∫ –õ–û–ö–ê–¶–ò–Ø"):
        markup = types.InlineKeyboardMarkup(row_width=1)
        button1 = types.InlineKeyboardButton("üó∫ google.maps!", url="https://goo.gl/maps/QfNy9vRTgutX2n6F9")  
        button2 = types.InlineKeyboardButton("üó∫ yandex.maps!", url="https://yandex.uz/maps/-/CCUVQFfCCA")
        markup.add(button1,button2)
        bot.send_message(message.chat.id, "<i><b>–£–í–ê–ñ–ê–ï–ú–´–ô(–∞—è)-***{0.first_name}***\n\nGOOGLE MAPS –î–õ–Ø ANDROID!\n\nYANDEX MAPS –î–õ–Ø IOS!\n</b></i>".format(message.from_user), reply_markup=markup,parse_mode="html") 
    elif (message.text == "üè® –ß–¢–û –ù–ê–•–û–î–ò–¢–°–Ø –†–Ø–î–û–ú"):
        bot.send_message(message.chat.id, text="<b>üè® –ß–¢–û –ù–ê–•–û–î–ò–¢–°–Ø –†–Ø–î–û–ú\n\n"
                                                                "‚úàÔ∏è–ê–≠–†–û–ü–û–†–¢ –∏–º.–ò–°–õ–ê–ú–ê –ö–ê–†–ò–ú–û–í–ê-4 –∫–∏–ª–æ–º–µ—Ç—Ä\n\n"
                                                                "‚òïÔ∏è–ö–ê–§–ï-500 –º–µ—Ç—Ä\n\n"
                                                                "üöá–ú–ï–¢–†–û –ú–ò–†–ó–û –£–õ–£–ì–ë–ï–ö-1 –∫–∏–ª–æ–º–µ—Ç—Ä\n\n"
                                                                "üß∫–°–£–ü–ï–† –ú–ê–†–ö–ï–¢-500 –º–µ—Ç—Ä\n\n"
                                                                "üïå–ú–ï–ß–ï–¢–¨-500 –º–µ—Ç—Ä\n\n"
                                                                "üé°–ü–ê–†–ö –∏–º.–ì–ê–§–£–†–ê –ì–£–õ–Ø–ú–ê-500 –º–µ—Ç—Ä\n\n"
                                                                "üèü–°–¢–ê–î–ò–û–ù:–ë–£–ù–Æ–î–ö–û–†-500 –º–µ—Ç—Ä\n\n"
                                                                "üöû–í–û–ö–ó–ê–õ –Æ–ñ–ù–´–ô-3 –∫–∏–ª–æ–º–µ—Ç—Ä\n\n"
                                                                "üöå–ê–í–¢–û–í–û–ö–ó–ê–õ-4 –∫–∏–ª–æ–º–µ—Ç—Ä\n\n</b>",parse_mode="html")                                                                              
    elif (message.text == "‚òéÔ∏è –ö–û–ù–¢–ê–ö–¢–´"):
        bot.send_message(message.chat.id, text="<b>üìù –ö–û–ù–¢–ê–ö–¢–´ –ì–û–°–¢–ò–ù–ò–¶–´</b>\n\n"
                                                                "<b>üìû–ù–û–ú–ï–† –ì–û–°–¢–ò–ù–ò–¶–´</b>\n+998712770707\n\n"
                                                                "<b>üì¨–°–í–Ø–ó–¨ –° RECEPTION</b>\n@hotel_diamond_reception\n\n"
                                                                "<b>üì¨–ü–û–ß–¢–ê –ì–û–°–¢–ò–ù–ò–¶–´</b>\ndiamond.tashkent.hotel@mail.ru\n\n"
                                                                "<b>üîµ–¢–ï–õ–ï–ì–†–ê–ú –ö–ê–ù–ê–õ –ì–û–°–¢–ò–ù–ò–¶–´</b>\nhttps://t.me/diamond_tashkent_hotel",parse_mode="html")
    elif (message.text == "–£–°–õ–£–ì–ò"):
        bot.send_message(message.chat.id, text="<i><b>–£–°–õ–£–ì–ò</b></i>\n\n"
                                                                "<b>1.–û–ë–©–ò–ï:\n</b>"
                                                                "<ins>–ë–µ—Å–ø–ª–∞—Ç–Ω–∞—è –ø–∞—Ä–∫–æ–≤–∫–∞ –Ω–∞ —Ç–µ—Ä—Ä–∏—Ç–æ—Ä–∏–∏</ins>\n\n"
                                                                "<b>2.–¢–†–ê–ù–°–ü–û–†–¢:</b>\n" 
                                                                "<ins>–¢—Ä–∞–Ω—Å—Ñ–µ—Ä –æ—Ç/–¥–æ –∞—ç—Ä–æ–ø–æ—Ä—Ç–∞, –¢—Ä–∞–Ω—Å—Ñ–µ—Ä (–æ–ø–ª–∞—á–∏–≤–∞–µ—Ç—Å—è –æ—Ç–¥–µ–ª—å–Ω–æ).</ins>\n\n"
                                                                "<b>3.–°–¢–û–ô–ö–ê –†–ï–ì–ò–°–¢–†–ê–¶–ò–ò:</b>\n" 
                                                                "<ins>–ö—Ä—É–≥–ª–æ—Å—É—Ç–æ—á–Ω–∞—è —Å—Ç–æ–π–∫–∞ —Ä–µ–≥–∏—Å—Ç—Ä–∞—Ü–∏–∏, –£—Å–∫–æ—Ä–µ–Ω–Ω–∞—è —Ä–µ–≥–∏—Å—Ç—Ä–∞—Ü–∏—è –∑–∞–µ–∑–¥–∞/–æ—Ç—ä–µ–∑–¥–∞, –•—Ä–∞–Ω–µ–Ω–∏–µ –±–∞–≥–∞–∂–∞.</ins>\n\n"  
                                                                "<b>4.–£–°–õ–£–ì–ê –£–ë–û–†–ö–ò:</b>\n" 
                                                                "<ins>–ï–∂–µ–¥–Ω–µ–≤–Ω–∞—è —É–±–æ—Ä–∫–∞.</ins>\n\n" 
                                                                "<b>5.–†–ê–ó–ù–û–ï:</b>\n" 
